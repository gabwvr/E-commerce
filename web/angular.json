{
  "$schema": "https://json.schemastore.org/angular-cli",
  "version": 1,
  "newProjectRoot": "./",
  "projects": {
    "ecommerceonline-web": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ecommerceonline-web",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["src/polyfills.ts"],
            "tsConfig": "tsconfig.json",
            "assets": [
              "src/assets",
              {
                "glob": "favicon.png",
                "input": "src/assets",
                "output": "/assets"
              }
            ],
            "styles": ["src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "sourceMap": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ecommerceonline-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ecommerceonline-web:build:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "ecommerceonline-web",
  "cli": {
    "analytics": false
  }
}
